{
    "name": "nuelcyoung/tenantable",
    "description": "Multitenant package for CodeIgniter 4 with multiple identification strategies, bootstrap system, and lifecycle events",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "codeigniter4",
        "multitenant",
        "subdomain",
        "saas",
        "tenant",
        "multitenancy"
    ],
    "homepage": "https://github.com/nuelcyoung/tenantable",
    "authors": [
        {
            "name": "Nuel Young Chukwunalu",
            "email": "nuelmega@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "codeigniter4/framework": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "nuelcyoung\\tenantable\\": "src/"
        },
        "files": [
            "src/Helpers/tenantable_helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "nuelcyoung\\tenantable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "codeigniter4": {
            "commands": [
                "nuelcyoung\\tenantable\\Commands\\TenantsList",
                "nuelcyoung\\tenantable\\Commands\\TenantsRun"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    }
}
